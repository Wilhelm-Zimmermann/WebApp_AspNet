@model WebAp.Models.Seller
@{
}

<div class="row">
    <div class="col-md-4 col-lg-12">
        <form asp-action="Create">
            <fieldset>
                <legend>New Seller</legend>
                <div class="form-group">
                    <label asp-for="Name">Name</label>
                    <input asp-for="Name" class="form-control" placeholder="Enter Name">
                </div>
                <div class="form-group">
                    <label asp-for="Email">Email</label>
                    <input asp-for="Email" class="form-control" placeholder="Enter Email">
                </div>
                <div class="form-group">
                    <label asp-for="BirthDate" class="control-label">BirthDate</label>
                    <input asp-for="BirthDate" class="form-control">
                </div>
                <div class="form-group">
                    <label asp-for="BasicSalary">BaseSalary</label>
                    <input asp-for="BasicSalary" class="form-control" placeholder="Enter BaseSalary">
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-outline-dark" />
                </div>
            </fieldset>
        </form>
    </div>
</div>

